module_config {
    module_library : "../bazel-bin/perception/modeld_component/libmodeld_component.so"
    components {
      class_name : "ModeldComponent"
      config {
        name : "ModeldComponent"
        readers {
          channel: "roadCameraState"
        },
        readers {
          channel: "lateralPlan"
        }
      }
    }
}

module_config {
    module_library : "../bazel-bin/perception/camerad_component/libcamerad_component.so"
    components {
      class_name : "CameradComponent"
      config {
        name : "camera_id_AR0233"
        config_file_path : "../perception/camerad_component/conf/camera_id_AR0233.pb.txt"
      }
    }
}

module_config {
    module_library : "../bazel-bin/perception/calibration_component/libcalibration_component.so"
    components {
      class_name : "CalibrationComponent"
      config {
        name : "CalibrationComponent"
        readers {
          channel: "CarState"
        }
      }
    }
}